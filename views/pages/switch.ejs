<!doctype html>
<html lang="pt-br">

    <%- include('../partials/head'); %>
    
    <body class="navbar-expand-lg">
        <%- include('../partials/header'); %>

        <div class="main d-flex">
            <%- include('../partials/navbar-aside.ejs'); %>

            <div class="container px-5">
                <h2 class="heading-2 brand-color-secondary-pure mt-5"><i class="bi bi-toggle-on"></i> Switch</h2>

                <div class="row">
                  <div class="col-lg-12 my-4">
                    <div class="cardes mb-5">
                      <div>
                        <div class="mb-3 switch-box">
                          <input type="checkbox" value="" id="default-input" checked>
                          <label class="form-check-label switch-box-label" for="default-input"> </label>
                        </div>
                        <div class="mb-3 switch-box">
                          <input type="checkbox" value="" id="default-input" checked>
                          <label class="form-check-label switch-box-label" for="default-input"> Default Primary </label>
                        </div>
                        
                        <div class="mb-3 switch-box switch-secondary">
                          <input type="checkbox" value="" id="info-input" checked>
                          <label class="form-check-label switch-box-label" for="info-input"> Secondary </label>
                        </div>
                        
                        <div class="mb-3 switch-box switch-success">
                          <input type="checkbox" value="" id="success-input" checked>
                          <label class="form-check-label switch-box-label" for="success-input"> Success </label>
                        </div>
                        
                        <div class="mb-3 switch-box switch-danger">
                          <input type="checkbox" value="" id="danger-input" checked>
                          <label class="form-check-label switch-box-label" for="danger-input"> Danger </label>
                        </div>
                        
                        <div class="mb-3 switch-box switch-warning">
                          <input type="checkbox" value="" id="warning-input" checked>
                          <label class="form-check-label switch-box-label" for="warning-input"> Warning </label>
                        </div>
                        
                        <div class="mb-3 switch-box">
                          <input type="checkbox" value="" id="disabled-input" disabled checked>
                          <label class="form-check-label switch-box-label" for="disabled-input"> Disabled </label>
                        </div>
                      </div>
                      <pre class="line-numbers mt-4">
                          <code class="language-markup">
                          </code>
                      </pre>
                    </div>
                  </div>




                </div>
            </div>
        </div>


        <%- include('../partials/footer'); %>
    </body>
</html>